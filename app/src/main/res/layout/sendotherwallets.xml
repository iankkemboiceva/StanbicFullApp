<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="left"
    android:orientation="vertical"
    android:background="@color/white"
    android:filterTouchesWhenObscured ="true"
    >



    <!--<ListView
        android:id="@+id/lv1"
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:background="@null"
        android:layout_height="wrap_content">

        </ListView>-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >


    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"

        android:layout_height="0dp"
        android:layout_weight="1"  >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"


        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"

android:id="@+id/layl"

            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:id="@+id/lylcal"

                android:orientation="vertical" >
                <RelativeLayout
                    android:id="@+id/lycal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/borderhmground"

                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/tv"
                        android:layout_height="40dp"

                        android:layout_width="40dp"

                        android:layout_marginTop="5dp"
                        android:text="2"
                        android:textColor="#fff"
                        android:textSize="20sp"
                        android:background="@drawable/bg_red"

                        android:layout_centerHorizontal="true"
                        android:gravity="center_vertical|center_horizontal"
                        />
                    <View
                        android:id="@+id/div1"
                        android:layout_width="30dp"

                        android:layout_height="1dp"
                        android:layout_marginTop="25dp"

                        android:layout_alignLeft="@+id/tv"

                        android:layout_marginLeft="50dp"

                        android:background="@color/textcolorcircle" />
                    <View
                        android:id="@+id/div2"
                        android:layout_width="30dp"

                        android:layout_height="1dp"
                        android:layout_marginTop="25dp"

                        android:layout_alignRight="@+id/tv"

                        android:layout_marginRight="50dp"

                        android:background="@color/textcolorcircle" />

                    <TextView
                        android:id="@+id/tv2"
                        android:layout_height="40dp"
                        android:layout_width="40dp"

                        android:layout_alignRight="@+id/div2"
                        android:layout_marginRight="40dp"
                        android:layout_marginTop="5dp"
                        android:text="1"
                        android:textColor="@color/textcolorcircle"
                        android:textSize="20sp"
                        android:background="@drawable/bg_transparent"

                        android:gravity="center_vertical|center_horizontal"
                        />
                    <TextView
                        android:id="@+id/tv3"
                        android:layout_height="40dp"

                        android:layout_width="40dp"

                        android:layout_marginTop="5dp"
                        android:text="3"
                        android:textColor="@color/textcolorcircle"
                        android:textSize="20sp"
                        android:background="@drawable/bg_transparent"

                        android:layout_alignLeft="@+id/div1"
                        android:layout_marginBottom="20dp"
                        android:layout_marginLeft="40dp"
                        android:gravity="center_vertical|center_horizontal"
                        />
                    <TextView
                        android:id="@+id/textV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"

android:visibility="gone"
                        android:layout_below="@+id/tv"
                        android:textSize="12sp"
                        android:layout_centerInParent="true"
                        android:text="Proceed to fill all fields"
                        android:textColor="#ffffff" />

                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rlag"
                    android:layout_width="match_parent"
                    android:layout_marginTop="10dp"

                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="5dp"
                        android:text="SEND TO MOBILE MONEY WALLET"
                        android:textColor="@color/colorPrimary"
                        android:textSize="14sp" />

                </RelativeLayout>
                <LinearLayout

                    android:layout_marginTop="10dp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="10dip"
                    android:paddingRight="10dip"
                    android:paddingBottom="10dip"
                    android:paddingTop="10dip"
android:background="@color/white"

                    >
                    <RelativeLayout


                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"



                        >
                        <TextView
                            android:id="@+id/textVip"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dip"
                            android:visibility="gone"
                            android:text="Select Telco Operator"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="@color/colorPrimary" />

                        <android.support.v7.widget.RecyclerView
                            android:id="@+id/listView2"
                            android:layout_width="match_parent"
                            android:visibility="gone"
                            android:layout_below="@+id/textVip"
                            android:layout_height="match_parent" />
                    </RelativeLayout>

                    <RelativeLayout


                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_margin="5dp"

  >
                        <TextView
                            android:id="@+id/textmmo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dip"
                            android:text="Mobile Money Operator"
                            android:textSize="12sp"
                            android:textColor="@color/colorPrimary" />
                        <TextView
                            android:id="@+id/textVipp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dip"
                            android:layout_alignParentRight="true"
                            android:text="Select Wallet ->"
                            android:textSize="12sp"
                            android:textColor="@color/nbkyellow" />
                 <!--       <TextView
                            android:id="@+id/bname"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="@color/colorPrimary"
                            android:text="Mobile Money Operator"


                            />
                        <Spinner
                            android:id="@+id/spin1"
                            style="@style/SpinnerAppTheme"
                            android:layout_below="@+id/bname"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                        </Spinner>-->
                    </RelativeLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:layout_weight="1.5"
                            android:text="Phone Number"
                            android:textColor="@color/black" />
                        <EditText
                            android:id="@+id/phonenumb"
                            android:background="@android:color/transparent"

                            android:textSize="14sp"
                            android:layout_margin="5dip"
                            android:ems="10"
                            android:maxLength="13"
                            android:hint="Phone Number"
                            android:inputType="number"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:layout_weight="2"
                            />

                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/divcolor" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:orientation="horizontal"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_height="45dp">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_weight="1.5"
                            android:layout_gravity="left|center"
                            android:textColor="@color/black"
                            android:text="Name"
                            android:layout_height="wrap_content" />
                        <EditText
                            android:id="@+id/cname"
                            android:inputType="none"
                            android:focusable="false"
                            android:layout_width="0dp"
                            android:layout_weight="2"
                            android:layout_gravity="left|center"
                            android:hint="Check Name"
                            android:textSize="12sp"
                            android:background="@android:color/transparent"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/divcolor" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:layout_weight="1.5"
                            android:text="Amount  (KES)"
                            android:textColor="@color/black" />

                        <EditText
                            android:id="@+id/amount"
                            android:layout_below="@+id/textView4"


                            android:ems="10"

                            android:inputType="numberDecimal"
                            android:layout_margin="10dp"
                            android:hint="Amount"


                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:layout_weight="2"
                            android:background="@android:color/transparent"

                            android:textSize="14sp"/>
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/divcolor" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:layout_weight="1.5"
                            android:text="Narration"
                            android:textColor="@color/black" />

                        <EditText
                            android:id="@+id/ednarr"
                            android:layout_below="@+id/textView5"


                            android:ems="10"

                            android:inputType="text"
                            android:layout_margin="10dp"
                            android:hint="Narration"

                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:layout_weight="2"
                            android:background="@android:color/transparent"

                            android:textSize="14sp"/>
                    </LinearLayout>
                    </LinearLayout>
                <LinearLayout


                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="10dp"

                    android:paddingLeft="10dip"
                    android:paddingRight="10dip"
                    android:paddingBottom="10dip"
                    android:paddingTop="10dip"
                    android:background="@color/white"

                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:layout_weight="1.5"
                            android:text="@string/sendname"
                            android:textColor="@color/black" />

                        <EditText
                            android:id="@+id/sendname"
                            android:layout_below="@+id/textView5"

                            android:digits="abcdefghijklmnopqrstuvwxyz ABCDEFGHIJKLMNOPQRSTUVWXYZ"

                            android:ems="10"

                            android:inputType="text"

                            android:hint="@string/sendname"


                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:layout_weight="2"
                            android:background="@android:color/transparent"

                            android:textSize="14sp"/>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/divcolor" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:layout_weight="1.5"
                            android:text="@string/sendnum"
                            android:textColor="@color/black" />

                        <EditText
                            android:id="@+id/sendnumber"
                            android:layout_below="@+id/textView5"
                            android:layout_margin="5dip"
                            android:digits="0123456789+"
                            android:ems="10"
                            android:maxLength="13"
                            android:inputType="number"

                            android:hint="@string/sendnum"


                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:layout_weight="2"
                            android:background="@android:color/transparent"

                            android:textSize="14sp"/>
                    </LinearLayout>

                </LinearLayout>



                <android.support.v7.widget.CardView
                    android:id="@+id/card_view9"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
android:visibility="gone"
                    android:layout_centerHorizontal="true"

                    android:layout_marginTop="10dip"
                    android:layout_marginBottom="10dip"
                    android:clickable="true"


                    card_view:cardBackgroundColor="@color/white"
                    card_view:cardCornerRadius="5dp"
                    card_view:cardElevation="5dp">

                    <LinearLayout


                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip"
                        android:paddingBottom="10dip"
                        android:paddingTop="10dip"


                        >




                        <RelativeLayout


                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"



                            >
                            <TextView
                                android:id="@+id/textViewpnam"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dip"
                                android:text="Enter Payer Name:"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#3C2415" />
                            <EditText
                                android:id="@+id/scodepnam"
                                android:layout_below="@+id/textViewpnam"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dip"
                                android:ems="10"

                                />
                        </RelativeLayout>

                        <RelativeLayout


                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"



                            >
                            <TextView
                                android:id="@+id/bnamebcd"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dip"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#3C2415"
                                android:text="Enter Last Name:"


                                />
                            <EditText
                                android:id="@+id/lnam"
                                android:layout_below="@+id/bnamebcd"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dip"
                                android:ems="10"

                                />
                        </RelativeLayout>



                        <RelativeLayout


                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"



                            >
                            <TextView
                                android:id="@+id/mobno"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dip"
                                android:text="Enter Mobile Number:"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#3C2415" />

                            <EditText
                                android:id="@+id/mobnn"
                                android:layout_below="@+id/mobno"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dip"
                                android:ems="10"

                                android:inputType="numberDecimal" />
                        </RelativeLayout>


                        <RelativeLayout


                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"



                            >
                            <TextView
                                android:id="@+id/idno"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dip"
                                android:text="Enter ID Number:"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#3C2415" />

                            <EditText
                                android:id="@+id/edidno"
                                android:layout_below="@+id/idno"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dip"
                                android:ems="10"

                                android:inputType="numberDecimal" />
                        </RelativeLayout>



                    </LinearLayout>


                </android.support.v7.widget.CardView>



                <Button
                    android:id="@+id/button2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"
                    android:background="@color/nbkyellow"

                    android:padding="5dp"
                    android:text="OK"
android:layout_marginBottom="20dp"
                    android:layout_marginTop="10dip"
                    android:textColor="@color/colorPrimary" />

                </LinearLayout>
</LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
</RelativeLayout>