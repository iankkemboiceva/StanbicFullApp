<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:filterTouchesWhenObscured ="true"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >


        <ScrollView
            android:id="@+id/scrollView1"
            android:layout_width="match_parent"
            android:layout_below = "@+id/container_toolbar"
            android:layout_marginLeft="15dip"
            android:layout_marginRight="15dip"
            android:layout_height="0dp"
            android:layout_weight="1" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"


                android:orientation="vertical" >



                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"

                    android:paddingBottom="10dip"

                    >
                    <RelativeLayout


                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_margin="5dp"
                        android:visibility="gone"



                        >

                        <TextView
                            android:id="@+id/bname"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#3C2415"
                            android:text="Select Account Type"


                            />

                    </RelativeLayout>
                    <RelativeLayout


                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
android:visibility="gone"


                        >

                        <TextView
                            android:id="@+id/tdispedit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dip"
                            android:layout_marginLeft="15dip"

                            android:text="What are the Account Types?"
                            android:textStyle="bold"

                            android:textColor="@color/blue" />



                    </RelativeLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        android:id="@+id/lylcal"

                        android:orientation="vertical" >
                        <android.support.v7.widget.CardView
                            android:id="@+id/card_view29"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"

                            android:layout_centerHorizontal="true"

                            android:layout_marginTop="10dip"

                            android:layout_marginRight="30dp"
                            android:layout_marginLeft="30dp"
                            android:clickable="true"
                            card_view:cardBackgroundColor="@color/colorPrimary"

                            card_view:cardCornerRadius="5dp"
                            card_view:cardElevation="5dp">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"


                                android:orientation="vertical" >
                                <TextView
                                    android:id="@+id/textV"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="5dip"
                                    android:layout_gravity="center_horizontal"
                                    android:text="Next of KIN Details"
                                    android:textSize="14sp"

                                    android:textColor="#ffffff" />

                            </LinearLayout>
                        </android.support.v7.widget.CardView>

                    </LinearLayout>
                    <RelativeLayout


                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp"


                        >
                        <CheckBox
                            android:id="@+id/chkbox"
                            android:visibility="gone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Allow Biometric Capture"/>
                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/input_layout_password"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <EditText
                                android:id="@+id/user_id2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="10dp"

                                android:hint="Next of Kin First Name">
                            <requestFocus />
                        </EditText>

                        </android.support.design.widget.TextInputLayout>


                    </RelativeLayout>
                    <RelativeLayout


                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"



                        >

                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/input_lksl"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <EditText
                                android:id="@+id/user_id29"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="10dp"
                                android:textColorHint="@color/colorPrimary"
                                android:hint="Next of KIN Middle Name" />

                        </android.support.design.widget.TextInputLayout>
                    </RelativeLayout>
                    <RelativeLayout


                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"



                        >

                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/input_layout_password2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <EditText
                                android:id="@+id/user_id232"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="10dp"
                                android:textColorHint="@color/colorPrimary"
                                android:hint="Next of KIN Last Name" />

                        </android.support.design.widget.TextInputLayout>
                    </RelativeLayout>
                    <RelativeLayout


                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"



                    >

                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/input_layout_password3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <EditText
                                android:id="@+id/user_id"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="10dp"
                                android:textColorHint="@color/colorPrimary"
                                android:hint="Next of KIN Relation" />

                        </android.support.design.widget.TextInputLayout>
                </RelativeLayout>
                    <RelativeLayout


                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"



                        >

                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/input_layout_password4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <EditText
                                android:id="@+id/user_id45"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="10dp"
                                android:textColorHint="@color/colorPrimary"
                                android:inputType="number"
                                android:hint="Next of KIN Mobile Number" />

                        </android.support.design.widget.TextInputLayout>
                    </RelativeLayout>

                    <RelativeLayout


                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"



                        >

                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/input_layout_password40"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <EditText
                                android:id="@+id/user_id40"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="10dp"
                                android:textColorHint="@color/colorPrimary"

                                android:hint="Next of KIN Address" />

                        </android.support.design.widget.TextInputLayout>
                    </RelativeLayout>

                    <RelativeLayout


                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"




                        >

                        <TextView
                            android:id="@+id/gnd"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="@color/colorPrimary"
                            android:text="Next of KIN Gender"
                            android:layout_marginLeft="15dp"

                            />
                        <Spinner
                            android:id="@+id/spin5"
                            style="@style/SpinnerAppTheme"
                            android:layout_below="@+id/gnd"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                        </Spinner>
                    </RelativeLayout>
                    <RelativeLayout


                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"

                        android:layout_marginLeft="15dp"

                        >
                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"


                            android:text="Enter Next of KIN Date Of Birth:"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="@color/colorPrimary" />



                        <Button
                            android:id="@+id/button4"
                            android:layout_width="40dp"
                            android:layout_height="40dp"


                            android:background="@drawable/icons18"
android:layout_below="@+id/textView7"



                            android:textColor="#ffffff" />
                        <TextView
                            android:id="@+id/bnameh"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:layout_below="@+id/textView7"
                            android:layout_alignParentRight="true"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="@color/colorPrimary"
                            android:text="N/A"
                            />
                    </RelativeLayout>

                    <Button
                        android:id="@+id/button1"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="15dip"
                        android:background="@color/nbkyellow"
                        android:textColor="@color/white"
                        android:text="Next" />

                </LinearLayout>





            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</LinearLayout>